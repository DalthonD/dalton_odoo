<odoo>
  <data>
    <template id ='company_strategiksv_landed_cost_report_pdf'>
      <t t-call="web.html_container">
        <t t-call="web.basic_layout">
          <div class="page" style="font-size: 9pt">
            <style type="text/css">
              .spacing-line{
                line-height: 5pt
              }
              .center-middle-title{
                font-size: 15px;
                text-transform: uppercase;
                line-height: 10pt;
              }
              .center-middle{
                text-align: center;
                vertical-align: middle !important;
                padding:0.5px !important;
                width70%;
              }
              .center-middle-o{
                text-align: center;
                vertical-align: middle !important;
                white-space: nowrap !important;
                line-height: 5pt;
              }
              .center-middle-c{
                text-align: center;
                vertical-align: middle !important;
                white-space: nowrap !important;
                padding:1px !important;
                width70%;
                color:white
              }
              .tr-color{
                background-color:blue;
                width:100%;
                color:white;
              }
              .table-content{
                text-align: right;
                vertical-align: middle;
                height: auto;
                width: 100%;
                padding: 0px;
                border-collapse: collapse;
              }
              table{
                width:100%;
              }
              table td{
                padding:0.5px;
                white-space: nowrap;
              }
              table td:last-child{
                padding: 0.5px;
                width:100%;
              }
              .pagenumber:before {
                position: relative;
                border-width: 55%;
                bottom: 1;
                width: 100%;
                font-size: 7pt;
                background-color:gray;
                content: "Pág. " conunter(page) " de " counter(pages);
              }
              .page:after{
                content: counter(page)
              }
            </style>
            <!--Declaracion de variables-->
            <t t-set="company_id" t-value="int(0)"/>
            <t t-set="contador" t-value="null"/>
            <t t-set="company" t-value="null"/>

            <t t-set="compra" t-value="float(0.00)"/>
            <t t-set="zero" t-value="float(0.00)"/>
            <t t-set="totalExentas" t-value="float(0.00)"/>
            <t t-set="totalGravadas" t-value="float(0.00)"/>
            <t t-set="totalIva" t-value="float(0.00)"/>
            <t t-set="totalRetenido" t-value="float(0.00)"/>
            <t t-set="totalPercibido" t-value="float(0.00)"/>
            <t t-set="totalCompras" t-value="float(0.00)"/>
            <t t-set="totalExcluidos" t-value="float(0.00)"/>
            <t t-set="totalRetenido3" t-value="float(0.00)"/>
            <t t-set="importacion" t-value="false"/>
            <t t-set="intEx" t-value="float(0.00)"/>
            <t t-set="impEx" t-value="float(0.00)"/>
            <t t-set="intGra" t-value="float(0.00)"/>
            <t t-set="impGra" t-value="float(0.00)"/>
            <!--Fin de declaraciones-->

            <!--Encabezado-->
              <t t-foreach="docs" t-as="o">
                <t t-set="company_id" t-value="int(data.get('form').get('company_id')[0])"/>
                <div class="row" style="width:100%">
                  <table style="table-layout:auto;border-collapse:collapse;empty-cells:hide;">
                    <tr>
                      <td style="text-align:left">
                          <div class="col-3 mb4" style="text-align:left">
                            <img t-if="o.company_id.logo" t-attf-src="data:image/*;base64,{{o.company_id.logo}}" style="max-height: 45px;"/>
                          </div>
                      </td>
                      <td class="center-middle" style="text-align:right">
                          <b><span style="font-size:20pt,color:blue;text-align:right">RETACEO</span></b>
                      </td>
                    </tr>
                  </table>
                </div>
                <table style="margin-top:8pt">
                  <tr>
                    <td>
                      <table class="table table-bordered" style="table-layout:auto;border:1px solid black;border-collapse:collapse;empty-cells:hide;">
                        <tr class="tr-color">
                          <th class="center-middle">Factura</th>
                          <th class="center-middle">Fecha</th>
                          <!--th class="center-middle">Referencia</th-->
                        </tr>
                        <tr t-foreach="o.get_invoices_inf()" t-as="t">
                          <td class="center-middle-o" style="text-align:left;width:50%"><span t-esc="t.get('referencia')"/></td>
                          <td class="center-middle-o" style="width:70%"><span t-esc="t.get('fecha')"/></td>
                          <!--td class="center-middle" style="text-align:right"><span t-esc=""/></td-->
                        </tr>
                      </table>
                    </td>
                    <td style="width:60%">
                    </td>
                    <td>
                      <table class="table table-bordered" style="table-layout:auto;border:1px solid black;border-collapse:collapse;empty-cells:hide;">
                        <tr class="tr-color">
                          <th colspan="2" class="center-middle" style="color:white;width:15%">Datos de la importación</th>
                        </tr>
                        <tr>
                          <td class="center-middle" style="text-align:left;width:30%">DECLARACION No</td>
                          <td class="center-middle" style="text-align:left;width:25%"><span t-esc="o.sv_declaracion"/></td>
                        </tr>
                        <tr>
                          <td class="center-middle" style="text-align:left;width:30%">FECHA DE IMPORT.</td>
                          <td class="center-middle" style="text-align:left;width:25%"><span t-esc="o.date"/></td>
                        </tr>
                        <tr>
                          <td class="center-middle" style="text-align:left;width:30%">GUIA/BL</td>
                          <td class="center-middle" style="text-align:left;width:25%"><span t-esc="o.sv_guia"/></td>
                        </tr>
                        <tr>
                          <td class="center-middle" style="text-align:left;width:30%">SHIP VIA</td>
                          <td class="center-middle" style="text-align:left;width:25%"></td>
                        </tr>
                      </table>
                    </td>
                  </tr>
                </table>
                <table>
                  <tr>
                    <td>
                      <table class="table table-bordered" style="table-layout:auto;border:1px solid black;border-collapse:collapse;empty-cells:hide;">
                        <tr class="tr-color">
                          <th class="center-middle-c">PRODUCTO#</th>
                          <th class="center-middle-c">DESCRIPCIÓN DEL PRODUCTO</th>
                          <th class="center-middle-c">DESCRIPCIÓN ADUANA</th>
                          <th class="center-middle-c">POSICIÓN ARANCELARIA</th>
                          <th class="center-middle-c">CANTIDAD</th>
                          <th class="center-middle-c">PRECIO UNITARIO</th>
                          <th class="center-middle-c">VALOR $ FOB</th>
                          <th class="center-middle-c">SEGURO FLETE</th>
                          <th class="center-middle-c">VALOR $ CIF</th>
                          <th class="center-middle-c">IMPUESTOS</th>
                          <th class="center-middle-c">OTROS GASTOS</th>
                          <th class="center-middle-c">TOTAL COSTO</th>
                          <th class="center-middle-c">COSTO UNITARIO</th>
                        </tr>
                        <tr>
                          <td></td>
                        </tr>
                        <tr>
                          <td></td>
                        </tr>
                      </table>
                    </td>
                    <td>
                    </td>
                  </tr>
                </table>

                <!--div t-if="report_type == 'pdf'" class="text-muted" style="position:absolute;bottom:5pt;text-align:left">
                    Pág.: <span class="page"/> de <span class="topage"/>
                </div-->
              </t>
          </div>
        </t>
      </t>
    </template>
  </data>
</odoo>
